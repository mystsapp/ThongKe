            $(function () {
                $('.modal-dialog').draggable();
            });
        </script>*@
}

<br />
<form id="frmSearch" asp-controller="BaoCao" asp-action="NgayDiTour" method="get" class="form-horizontal" role="form">
    <div class="row">
        <div class="col-md-12">


            <div class="col-md-4">
                Từ ngày
                <input type="text" class="form-control" name="tungay" id="txtTuNgay" placeholder="Type date..." />
            </div>

            <div class="col-md-4">
                Dến ngày
                <input type="text" class="form-control" name="denngay" id="txtDenNgay" placeholder="Type date..." />
            </div>

            <div class="col-md-2">
                <br />
                <button id="btnSearch" class="btn btn-primary"><i class="fa fa-search"></i></button>
                <button id="btnReset" class="btn btn-secondary">Reset</button>
                @* <button id="btnAddNew" class="btn btn-success"><i class="fa fa-plus-square"></i></button>*@
            </div>


            @*<div class="col-md-2">
                    <button id="btnAddNew" class="btn btn-success pull-right"><i class="fa fa-plus-square"></i>Add new</button>
                </div>*@

        </div>
    </div>
</form>
